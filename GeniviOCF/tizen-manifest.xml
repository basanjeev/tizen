    <feature name="http://tizen.org/feature/sensor.linear_acceleration">true</feature>
    <feature name="http://tizen.org/feature/sensor.accelerometer.wakeup">true</feature>
    <feature name="http://tizen.org/feature/sensor.gesture_recognition">true</feature>
    <feature name="http://tizen.org/feature/sensor.tiltmeter">true</feature>
