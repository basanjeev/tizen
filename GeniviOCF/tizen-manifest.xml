<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="2.3.2" package="org.openconnectivity.geniviocf" version="1.0.0">
    <author email="staff@openconnectivity.com" href="www.openconnectivity.org">Open Connectivity Foundation</author>
    <description>Demo app for running the automotive demo at CES 2017.</description>
    <profile name="wearable"/>
    <ui-application appid="org.openconnectivity.geniviocf" exec="geniviocf" multiple="false" nodisplay="false" taskmanage="true" type="capp">
        <label>GeniviOCF</label>
        <icon>geniviocf</icon>
